  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.14",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17"
  },
