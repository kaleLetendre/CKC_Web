{
  "name": "ckc-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i ./public/stylesheets/style.css -o ./public/stylesheets/output.css",
    "watch:css": "tailwindcss -i ./public/stylesheets/style.css -o ./public/stylesheets/output.css --watch",
    "start": "node app.js",
    "dev": "concurrently \"npm run watch:css\" \"npm start\""
